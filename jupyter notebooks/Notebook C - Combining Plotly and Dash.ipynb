    "        fill='tozeroy',\n",
    "        fillcolor='tomato',\n",
    "        line = dict(\n",
    "            color='firebrick',\n",
    "            width=2),\n",
    "        mode='lines',  # you can also use \"lines+markers\", or just \"markers\"\n",
    "        name='Drawdown %'))\n",
    "\n",
    "drawdown_chart.update_layout(\n",
    "    margin = dict(t=45, b=30, l=25, r=25),\n",
    "    yaxis=dict(\n",
