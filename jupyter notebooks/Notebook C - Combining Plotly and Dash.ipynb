    "    fig_main.add_trace(go.Scatter(\n",
    "        x=tx_df[tx_df.type=='Buy'].date,\n",
    "        y=tx_df[tx_df.type=='Buy'].price,\n",
    "        mode='markers',\n",
    "        name='Buy Orders',\n",
    "        marker=dict(\n",
    "            color='rgba(60, 255, 75, 0.8)',\n",
    "            size=12,\n",
    "            line=dict(\n",
    "                color='white',\n",
    "                width=1\n",
